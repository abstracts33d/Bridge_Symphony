extends layout.pug

block content
	+carousel("presentation",[{image:"../images/A1b.jpg",h1:carousel_h1_1,button:{caption:carousel_caption_1,url:"#F1"}},{image:"../images/B2b.jpg",h1:carousel_h1_2,button:{caption:carousel_caption_2,url:"#F2"}},	{image:"../images/C3b.jpg",h1:carousel_h1_3,button:{caption:carousel_caption_3,url:"#F3"}}])

	.container.marketing
		p#F1
		hr.featurette-divider
		.row.featurette
			.col-md-7
				h2(class='featurette-heading') #{feature1_h2}
					span.text-muted #{feature1_h2_sub}
				p.lead #{feature1_p}

			.col-md-5
				<img class="featurette-image img-responsive center-block" data-src="holder.js/500x500/auto" alt="500x500" src="data:image/svg+xml;base64," data-holder-rendered="true">

		p#F2
		hr.featurette-divider
		.row.featurette
			.col-md-7
				h2(class='featurette-heading') #{feature2_h2}
					span.text-muted #{feature2_h2_sub}
				p.lead #{feature2_p}

			.col-md-5.col-md-pull-7
				<img class="featurette-image img-responsive center-block" data-src="holder.js/500x500/auto" alt="500x500" src="data:image/svg+xml;base64," data-holder-rendered="true">

		p#F3
		hr.featurette-divider
		.row.featurette
			.col-md-7
				h2(class='featurette-heading') #{feature3_h2}
					span.text-muted #{feature3_h2_sub}
				p.lead #{feature3_p}

			.col-md-5
				<img class="featurette-image img-responsive center-block" data-src="holder.js/500x500/auto" alt="500x500" src="data:image/svg+xml;base64," data-holder-rendered="true">


		hr.featurette-divider

		br
		br
		br
		br
		br
		p
